    "Firebase": {
      "cats": [
        34,
        47
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "headers.vary": {
                  "$regex": "x-fh-requested-host"
                }
              },
              {
                "body": {
                  "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*/(?:([\\d.]+)/)?firebase(?:\\.min)?\\.js\\;version:\\1"
                }
              },
              {
                "body": {
                  "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*/firebasejs/([\\d.]+)/firebase\\;version:\\1"
                }
              },
              {
                "body": {
                  "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*\\.gstatic\\.com/firebasejs/([\\d\\.]+)/\\;version:\\1"
                }
              },
              {
                "body": {
                  "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*firebase(?:config|io\\.com)"
                }
              }
            ]
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof firebase !== 'undefined' \u0026\u0026 typeof firebase.SDK_VERSION !== 'undefined'; } catch(e){ return ''; }",
          "version": "try { return firebase.SDK_VERSION || \"\"; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof firebase.SDK_VERSION !== 'undefined';",
          "version": "try { return String(firebase.SDK_VERSION || ''); } catch(e){ return ''; }"
        }
      ],
      "description": "Firebase is a Google-backed application development software that enables developers to develop iOS, Android and Web apps.",
      "website": "https://firebase.google.com",
      "icon": "Firebase.svg",
      "cpe": "cpe:2.3:a:google:firebase_cloud_messaging:*:*:*:*:*:*:*:*"
    },
