    "UniFi OS": {
      "cats": [
        28
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof unifiConstant !== 'undefined' \u0026\u0026 typeof unifiConstant.VERSION !== 'undefined'; } catch(e){ return ''; }",
          "version": "try { return unifiConstant.VERSION || \"\"; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof unifiConstant.VERSION !== 'undefined';",
          "version": "try { return String(unifiConstant.VERSION || ''); } catch(e){ return ''; }"
        }
      ],
      "description": "UniFi OS is the operating system for UniFi products, which provides a user interface.",
      "website": "https://www.ui.com/",
      "icon": "UniFi_OS.png"
    },
