          }
        }
      ],
      "description": "Fedora is a free open-source Linux-based operating system.",
      "website": "https://fedoraproject.org",
      "icon": "Fedora.png",
      "cpe": "cpe:2.3:o:fedoraproject:fedora:*:*:*:*:*:*:*:*"
    },
