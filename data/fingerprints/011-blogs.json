    "Aegea": {
      "cats": [
        11
      ],
      "implies": [
        "PHP",
        "jQuery"
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "headers.x-powered-by": {
              "$regex": "^(?:e2\\s)?aegea\\s(?:v)?([\\d\\.]+)\\;version:\\1"
            }
          }
        }
      ],
      "description": "Aegea is a blog engine created by Ilya Birman.",
      "website": "https://blogengine.ru",
      "icon": "Aegea.png"
    },
