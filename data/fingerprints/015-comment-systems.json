      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "body": {
              "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*\\.annoto\\.io"
            }
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof Annoto !== 'undefined' \u0026\u0026 typeof Annoto.boot !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof Annoto.boot !== 'undefined';"
        },
        {
          "path": "/",
          "detection": "return typeof AnnotoApiPromise !== 'undefined';"
        }
      ],
