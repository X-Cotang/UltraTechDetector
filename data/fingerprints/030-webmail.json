            "$or": [
              {
                "body": {
                  "$regex": "\u003clink[^\u003e]+/owa/auth/([\\d\\.]+)/themes/resources\\;version:\\1"
                }
              },
              {
                "headers.x-owa-version": {
                  "$regex": "([\\d\\.]+)?\\;version:\\1"
                }
              }
            ]
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof IsOwaPremiumBrowser !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof IsOwaPremiumBrowser !== 'undefined';"
        }
      ],
