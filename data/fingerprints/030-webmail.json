    "RainLoop": {
      "cats": [
        30
      ],
      "implies": [
        "PHP"
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "body": {
                  "$regex": "\u003clink[^\u003e]href=\"rainloop/v/([0-9.]+)/static/apple-touch-icon\\.png/\u003e\\;version:\\1"
                }
              },
              {
                "headers.server": {
                  "$regex": "^rainloop"
                }
              },
              {
                "body": {
                  "$regex": "\u003cmeta[^\u003e]+name=[\"']rlappversion[\"'][^\u003e]+content=[\"']^([0-9.]+)$\\;version:\\1[\"']"
                }
              },
              {
                "body": {
                  "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*^rainloop/v/([0-9.]+)/\\;version:\\1"
                }
              }
            ]
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof rainloop !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof rainloop !== 'undefined';"
        },
        {
          "path": "/",
          "detection": "return typeof rainloopI18N !== 'undefined';"
        }
      ],
      "description": "RainLoop is a web-based email client.",
      "website": "https://www.rainloop.net/",
      "icon": "RainLoop.png"
    },
