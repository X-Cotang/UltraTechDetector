    "Evernote": {
      "cats": [
        50
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "body": {
              "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*static\\.evernote\\.com"
            }
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof Evernote !== 'undefined' \u0026\u0026 typeof Evernote.Ajax !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof Evernote.Ajax !== 'undefined';"
        }
