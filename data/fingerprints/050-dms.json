{
  "apps": {
    "Clicksign": {
      "cats": [
        50
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof Clicksign !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof Clicksign !== 'undefined';"
        },
        {
          "path": "/",
          "detection": "return typeof endpointClickSign !== 'undefined';"
        }
      ],
      "description": "Clicksign is a platform offering document signing services.",
      "website": "https://www.clicksign.com",
      "icon": "Clicksign.svg"
    },
    "Clinked": {
      "cats": [
        50
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "body": {
              "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*a\\.clinked\\.com/(\\d+\\.\\d+\\.\\d+)/standard/js/.+\\.js\\;version:\\1"
            }
          }
        }
      ],
      "description": "Clinked is a cloud-based platform for secure file sharing, project management, and team collaboration with client portals.",
      "website": "https://clinked.com",
      "icon": "Clinked.svg"
    },
    "DSpace": {
      "cats": [
        50
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "body": {
              "$regex": "\u003cmeta[^\u003e]+name=[\"']generator[\"'][^\u003e]+content=[\"']dspace ([\\d.]+)\\;version:\\1[\"']"
            }
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof DSpaceChoiceLookup !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof DSpaceChoiceLookup !== 'undefined';"
        },
        {
          "path": "/",
          "detection": "return typeof DSpaceChoicesSetup !== 'undefined';"
        }
      ],
      "description": "DSpace is a software platform for academic, non-profit, and commercial organizations to create and manage open digital repositories.",
      "website": "https://dspace.org/",
      "icon": "DSpace.svg"
    },
    "Data8": {
      "cats": [
        50
      ],
      "browser": [
        {
