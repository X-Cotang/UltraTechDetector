      "website": "https://evernote.com",
      "icon": "Evernote.svg"
    },
    "Invenio": {
      "cats": [
        50
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "body": {
                  "$regex": "(?:powered by|system)\\s+(?:cern )?\u003ca (?:class=\"footer\" )?href=\"http://(?:cdsware\\.cern\\.ch(?:/invenio)?|invenio-software\\.org|cern\\.ch/invenio)(?:/)?\"\u003e(?:cds )?invenio\u003c/a\u003e\\s*v?([\\d\\.]+)?\\;version:\\1"
                }
              }
            ]
          }
        }
      ],
      "description": "Invenio is an open-source software framework for large-scale digital repositories that provides the tools for management of digital assets in an institutional repository and research data management systems.",
      "website": "https://invenio-software.org",
      "icon": "Invenio.png"
    },
    "Koha": {
      "cats": [
        50
      ],
      "implies": [
        "Perl"
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "body": {
                  "$regex": "\u003ca href=\"/cgi-bin/koha/"
                }
              },
              {
                "body": {
                  "$regex": "\u003cinput name=\"koha_login_context\" value=\"intranet\" type=\"hidden\"\u003e"
                }
              },
              {
                "body": {
                  "$regex": "\u003cmeta[^\u003e]+name=[\"']generator[\"'][^\u003e]+content=[\"']^koha ([\\d.]+)$\\;version:\\1[\"']"
                }
              }
            ]
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof KOHA !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof KOHA !== 'undefined';"
        }
      ],
      "description": "Koha is an open-source Integrated Library System (ILS).",
      "website": "https://koha-community.org/",
      "icon": "koha.png",
      "cpe": "cpe:2.3:a:koha:koha:*:*:*:*:*:*:*:*"
    },
