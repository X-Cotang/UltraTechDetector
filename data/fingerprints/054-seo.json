    "All in One SEO": {
      "cats": [
        54,
        87
      ],
      "implies": [
        "WordPress"
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "body": {
                  "$regex": "\u003c!-- all in one seo ([\\d.]+) \\;version:\\1"
                }
              },
              {
                "body": {
                  "$regex": "\u003c!-- all in one seo pack ([\\d.]+) \\;version:\\1"
                }
              },
              {
                "body": {
                  "$regex": "\u003c!-- all in one seo pro ([\\d.]+) \\;version:pro \\1"
                }
              },
              {
                "body": {
                  "$regex": "\u003cmeta[^\u003e]+name=[\"']generator[\"'][^\u003e]+content=[\"']^all in one seo \\(aioseo\\) ([\\d\\.]+)\\;version:\\1[\"']"
                }
              },
              {
                "body": {
                  "$regex": "\u003cmeta[^\u003e]+name=[\"']generator[\"'][^\u003e]+content=[\"']^all in one seo pro \\(aioseo\\) ([\\d\\.]+)\\;version:pro \\1[\"']"
                }
              }
            ]
          }
        }
      ],
      "description": "All in One SEO optimizes a WordPress website and its content for search engines.",
      "website": "https://aioseo.com",
      "icon": "AIOSEO.svg",
      "cpe": "cpe:2.3:a:aioseo:all_in_one_seo:*:*:*:*:*:wordpress:*:*"
    },
