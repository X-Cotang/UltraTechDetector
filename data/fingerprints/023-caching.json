{
  "apps": {
    "FastPixel": {
      "cats": [
        23,
        87
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "body": {
              "$regex": "\u003clink[^\u003e]+href=\"https://cdn\\.fastpixel\\.io"
            }
          }
        }
      ],
      "description": "FastPixel is a web performance and security service that provides caching, a content delivery network, web application firewall, and image optimization.",
      "website": "https://fastpixel.io/",
      "icon": "FastPixel.svg"
    },
    "Google PageSpeed": {
      "cats": [
        23,
        33,
        92
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "headers.x-mod-pagespeed": {
                  "$regex": "([\\d.]+)\\;version:\\1"
                }
              },
              {
                "headers.x-page-speed": {
                  "$regex": "(.+)\\;version:\\1"
                }
              }
            ]
          }
        }
      ],
      "description": "Google PageSpeed is a family of tools designed to help websites performance optimisations.",
      "website": "https://developers.google.com/speed/pagespeed/mod",
      "icon": "Google PageSpeed.svg"
    },
    "LiteSpeed Cache": {
      "cats": [
        23,
        87
      ],
      "implies": [
        "LiteSpeed"
      ],
      "paths": [
        {
          "path": "/",
          "detect": {
            "$or": [
              {
                "headers.x-litespeed-cache": {
                  "$exists": true
                }
              },
              {
                "headers.x-turbo-charged-by": {
                  "$regex": "litespeed"
                }
              },
              {
                "body": {
                  "$regex": "\u003cscript[^\u003e]+src=['\"][^'\"]*wp-content/plugins/litespeed-cache/"
                }
              }
            ]
          }
        }
      ],
      "browser": [
        {
          "path": "/",
          "detection": "try { return typeof litespeed_docref !== 'undefined'; } catch(e){ return ''; }"
        },
        {
          "path": "/",
          "detection": "return typeof litespeed_docref !== 'undefined';"
        }
      ],
      "description": "LiteSpeed Cache is an all-in-one site acceleration plugin for WordPress.",
      "website": "https://www.litespeedtech.com/products/cache-plugins/wordpress-acceleration",
      "icon": "LiteSpeed.svg"
    },
